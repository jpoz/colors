[global_config]
  title_transmit_bg_color = "#E87364"
  title_inactive_bg_color = "#526068"
  focus = system
[keybindings]
[profiles]
  [[default]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#242728:#E87364:#93D4AC:#F8D0A0:#43A9CF:#9581AD:#81B5BC:#aebabf:#526068:#E87364:#93D4AC:#F8D0A0:#43A9CF:#9581AD:#81B5BC:#f6f8f8"
    foreground_color = "#3c474c"
    background_color = "#f6f8f8"
    cursor_color = "#3c474c"

  [[Base16 Simple Light]]
    background_image = None
    copy_on_selection = True
    allow_bold = False
    palette = "#f6f8f8:#E87364:#93D4AC:#F8D0A0:#43A9CF:#9581AD:#81B5BC:#aebabf:#526068:#E87364:#93D4AC:#F8D0A0:#43A9CF:#9581AD:#81B5BC:#242728"
    foreground_color = "#3c474c"
    background_color = "#f6f8f8"
    cursor_color = "#3c474c"

[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = Base16 Simple Light
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
