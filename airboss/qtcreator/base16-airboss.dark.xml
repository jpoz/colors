<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base 16 Airboss Dark">

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#9CA299" background="#2B2D2E"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#959595"/>
  <style name="CurrentLine" background="#82898D"/>
  <style name="LineNumber" foreground="#82898D"/>
  <style name="CurrentLineNumber" foreground="#959595" bold="true"/>
  <style name="SearchResult" background="393C3D"/>
  <style name="Parentheses" background="#565A5C" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#F0F0F0" background="#82898D"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#82898D"/>
  <style name="Occurrences.Rename" background="#959595"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#FFAA91"/>

  <!-- language primitives -->
  <style name="Number" foreground="#FFAA91"/>
  <style name="String" foreground="#8CE071"/>
  <style name="Keyword" foreground="#7B0051"/>
  <style name="Operator" foreground="#9CA299"/>
  <style name="Preprocessor" foreground="#007A87"/>
  <style name="Label" foreground="#FF5A5F"/>

  <!-- types, classes -->
  <style name="Type" foreground="#FFB400"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#007A87"/>
  <style name="VirtualMethod" foreground="#007A87" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#CDD0CC"/>
  <style name="Local" foreground="#9CA299"/>
  <style name="Static" foreground="#9CA299"/>
  <style name="JsScopeVar" foreground="#9CA299" italic="true"/>
  <style name="JsImportVar" foreground="#9CA299" italic="true"/>
  <style name="JsGlobalVar" foreground="#9CA299" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#565A5C"/>

  <!-- comments -->
  <style name="Comment" foreground="#82898D"/>
  <style name="DisabledCode" foreground="#82898D"/>
  <style name="Doxygen.Comment" foreground="#82898D"/>
  <style name="Doxygen.Tag" foreground="#82898D"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#7B0051"/>
  <style name="Binding" foreground="#FF5A5F"/>
  <style name="QmlExternalId" foreground="#007A87" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#007A87" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#7B0051"/>
  <style name="DiffLocation" foreground="#007A87"/>
  <style name="AddedLine" foreground="#8CE071"/>
  <style name="RemovedLine" foreground="#FFB400"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#7B0051"/>
  <style name="DiffContextLine" foreground="#007A87"/>
  <style name="DiffSourceLine" foreground="#8CE071"/>
  <style name="DiffSourceChar" foreground="#FFB400" background="#393C3D"/>
  <style name="DiffDestLine" foreground="#8CE071"/>
  <style name="DiffDestChar" foreground="#FFB400" background="#393C3D"/>
</style-scheme>
